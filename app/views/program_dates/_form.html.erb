<div class="bigContainer">
<%= simple_form_for @program_date, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :program_location_id %>
  <%= f.input :date %>
  <%= f.input :status %>
  <%= f.input :comments %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                program_dates_path, :class => 'btn' %>
  </div>
<% end %>
</div>