<%- model_class = ProgramEnrollment -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:program_location_id) %></th>
      <th><%= model_class.human_attribute_name(:student_id_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @program_enrollments.each do |program_enrollment| %>
      <tr>
        <td><%= link_to program_enrollment.id, program_enrollment_path(program_enrollment) %></td>
        <td><%= program_enrollment.program_location_id %></td>
        <td><%= program_enrollment.student_id_id %></td>
        <td><%=l program_enrollment.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_program_enrollment_path(program_enrollment), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      program_enrollment_path(program_enrollment),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_program_enrollment_path,
            :class => 'btn btn-primary' %>
